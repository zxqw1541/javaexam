<?xml version="1.0" encoding="UTF-8"?>

<!-- 5) 엘리먼트의 개수 지정
  => 개수를 지정하지 않으면 필수
  => ? : 0 또는 1
  => * : 0 이상
  => + : 1 이상
-->
<!ELEMENT student (name,age?,tel+,address*)>
<!ELEMENT name    (#PCDATA)>
<!ELEMENT age     (#PCDATA)>
<!ELEMENT tel     (#PCDATA)>
<!ELEMENT address (#PCDATA)>

